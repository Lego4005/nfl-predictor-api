        "summary": "Rotoworld Player News - by Date",
        "parameters": [
          {
            "name": "format",
            "in": "path",
            "description": "Desired response format. Valid entries are <code>XML</code> or <code>JSON</code>.",
            "required": true,
            "type": "string",
            "default": "XML",
            "enum": [
              "XML",
              "JSON"
            ]
          },
          {
            "name": "date",
            "in": "path",
            "description": "The date of the news.<br>Examples: <code>2022-JUL-31</code>, <code>2022-SEP-01</code>.",
            "required": true,
            "type": "string"
          }
        ],
        "responses": {
          "200": {
            "description": "null",
            "schema": {
              "type": "array",
              "items": {
                "$ref": "#/definitions/RotoworldPlayerNewsItem"
              }
            }
          }
        },
        "produces": [
          "application/json"
        ]
      }
    },
