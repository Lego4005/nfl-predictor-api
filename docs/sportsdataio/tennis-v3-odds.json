  "paths": {
    "/{format}/MatchOddsByDate/{date}": {
      "get": {
        "description": "Returns the most recently available pre-match odds for tennis matches on the given date.",
        "operationId": "MatchOddsByDate",
        "summary": "Match Odds - by Date",
        "parameters": [
          {
            "name": "format",
            "in": "path",
            "description": "Desired response format. Valid entries are <code>XML</code> or <code>JSON</code>.",
            "required": true,
            "type": "string",
            "default": "XML",
            "enum": [
              "XML",
              "JSON"
            ]
          },
          {
            "name": "date",
            "in": "path",
            "description": "The date for which to pull matches. Ex: <code> 2023-01-03 </code>",
            "required": true,
            "type": "string"
          }
        ],
        "responses": {
          "200": {
            "description": "null",
            "schema": {
              "type": "array",
              "items": {
                "$ref": "#/definitions/Match"
              }
            }
          }
        },
        "produces": [
          "application/json"
        ]
      }
    },
    "/{format}/MatchOddsBySeasonAndDate/{seasonid}/{date}": {
      "get": {
        "description": "Returns the most recently available pre-match odds for tennis matches on the given date for a corresponding season record.",
        "operationId": "MatchOddsBySeasonByDate",
        "summary": "Match Odds by Season - by Date",
        "parameters": [
          {
            "name": "format",
            "in": "path",
            "description": "Desired response format. Valid entries are <code>XML</code> or <code>JSON</code>.",
            "required": true,
            "type": "string",
            "default": "XML",
            "enum": [
              "XML",
              "JSON"
            ]
          },
          {
            "name": "seasonid",
            "in": "path",
            "description": "The SeasonId for which to filter data. Example: <code>27</code>",
            "required": true,
            "type": "string"
          },
          {
            "name": "date",
            "in": "path",
            "description": "The date for which to pull matches. Ex: <code> 2023-01-03 </code>",
            "required": true,
            "type": "string"
          }
        ],
        "responses": {
          "200": {
            "description": "null",
            "schema": {
              "type": "array",
              "items": {
                "$ref": "#/definitions/Match"
              }
            }
          }
        },
        "produces": [
          "application/json"
        ]
      }
    },
    "/{format}/MatchOddsLineMovement/{matchid}": {
      "get": {
        "description": "Returns the full pre-match odds for given match, including all line movement history. This endpoint has a longer cache as it is meant for historical data/line movement rather than the most up to the second line.",
        "operationId": "MatchOddsLineMovement",
        "summary": "Match Odds Line Movement",
        "parameters": [
          {
            "name": "format",
            "in": "path",
            "description": "Desired response format. Valid entries are <code>XML</code> or <code>JSON</code>.",
            "required": true,
            "type": "string",
            "default": "XML",
            "enum": [
              "XML",
              "JSON"
            ]
          },
          {
            "name": "matchid",
            "in": "path",
            "description": "The MatchId for which to pull details.  Ex: <code> 2 </code>",
            "required": true,
            "type": "string"
          }
        ],
        "responses": {
          "200": {
            "description": "null",
            "schema": {
              "type": "array",
              "items": {
                "$ref": "#/definitions/Match"
              }
            }
          }
        },
        "produces": [
          "application/json"
        ]
      }
    },
