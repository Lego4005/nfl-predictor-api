    "/query/games/{game_id}": {
      "post": {
        "description": "The advanced query engine powered by Baker, allowing for conditional queries.",
        "operationId": "BakerAdvancedQueryGame",
        "summary": "BAKER Advanced Query Game",
        "parameters": [
          {
            "name": "game_id",
            "in": "path",
            "description": "The GameId of the event. This GameId is the SportsDataIO GameId.",
            "required": true,
            "type": "string"
          },
          {
            "name": "body",
            "in": "body",
            "schema": {
              "example": {
                "select": [
                  {
                    "category": "game",
                    "metric": "spread",
                    "measure": "average",
                    "period": "final",
                    "player_id": 0,
                    "value": 0
                  }
                ],
                "where": [
                  {
                    "category": "game",
                    "metric": "spread",
                    "period": "final",
                    "operator": "gt",
                    "value": 0,
                    "player_id": 0
                  }
                ]
              }
            },
            "description": "The body sent to tell the BAKER API which fields to return and the conditions to use when filtering."
          }
        ],
        "consumes": [
          "application/json"
        ],
        "responses": {
          "200": {
            "description": "",
            "schema": {
              "type": "null",
              "x-nullable": false
            }
          },
          "404": {
            "description": ""
          },
          "422": {
            "description": ""
          }
        },
        "produces": [
          "application/json"
        ]
      }
    },
